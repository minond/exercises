/**
 * Demultiplexor:
 *
 * if (sel == 0)
 *   {a, b} = {in, 0}
 * else
 *   {a, b} = {0, in}
 *
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    Not (in = sel, out = notSel);
    And (a = in, b = notSel, out = selA);
    And (a = sel, b = in, out = selB);
    And (a = in, b = notSel, out = a);
    And (a = in, b = sel, out = b);
}
